{% use 'form_theme_help.html.twig' %}

{%- block form_errors -%}
    {%- if errors|length > 0 -%}
        {%- for error in errors -%}
            <div>
                <label></label>
                <span>{{ error.message }}</span>
            </div>
        {%- endfor -%}
        <label></label>
    {%- endif -%}
{%- endblock form_errors -%}
